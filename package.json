{
  "name": "binance-futures-api",
  "version": "0.1.0",
  "description": "Futures API for Binance market",
  "main": "./dist/src/index.js",
  "scripts": {
    "clean": "rimraf coverage dist tmp",
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "start": "node dist/src/index.js",
    "dev": "ts-node-dev --respawn src/index.ts",
    "lint": "eslint . --ext .ts",
    "test": "dotenv -e .env.test jest --coverage",
    "test:watch": "jest --watch",
    "format": "prettier --write \"**/*.{js,ts}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coded-bear/binance-futures-api.git"
  },
  "keywords": [
    "Binance futures API"
  ],
  "author": "Łukasz Warchoł",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/coded-bear/binance-futures-api/issues"
  },
  "homepage": "https://github.com/coded-bear/binance-futures-api#readme",
  "dependencies": {
    "axios": "^0.26.1",
    "crypto": "^1.0.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "dotenv-cli": "^5.0.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.1",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2"
  }
}
